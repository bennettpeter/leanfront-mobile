<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--Words to be stripped from the front of titles for sorting
        Must be in UPPER CASE-->
    <string-array name="title_sort_articles">
        <item>@string/sort_exclude_1</item>
        <item>@string/sort_exclude_2</item>
        <item>@string/sort_exclude_3</item>
    </string-array>

    <string-array name="decode_pref_entries">
        <item>@string/pref_decode_auto</item>
        <item>@string/pref_decode_mediacodec</item>
        <item>@string/pref_decode_ffmpeg</item>
    </string-array>

    <string-array name="decode_values">
        <item>auto</item>
        <item>mediacodec</item>
        <item>ffmpeg</item>
    </string-array>

    <string-array name="commskip_pref_entries">
        <item>@string/menu_commskip_off</item>
        <item>@string/menu_commskip_notify</item>
        <item>@string/menu_commskip_skip</item>
    </string-array>

    <!-- 0=off, 1=notify, 2=skip   -->
    <string-array name="commskip_values">
        <item>0</item>
        <item>1</item>
        <item>2</item>
    </string-array>

    <string-array name="menu_commplaying">
        <item>@string/menu_comm_skip</item>
        <item>@string/menu_comm_notskip</item>
    </string-array>

